<% _.each(data,function(item,index){ %>
<tr class="<%=index%2==0?'':'gray'%>">
    <td class="th1"><%=item.name%></td>
    <td class="th2"><%=item.blmoney%></td>
    <td class="th3"><%=item.income%></td>
    <td class="th4"><%=item.expense%></td>
    <td class="th5"><%=item.elmoney%></td>
    <td class="th6"><a href="javascript:void(0)" reseller_id="<%=item.reseller_id%>" reseller_name="<%=item.name%>" class="detail_btn">详细</a></td>
</tr>
<%})%>