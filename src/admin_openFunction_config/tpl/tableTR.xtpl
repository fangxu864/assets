<% _.each(data,function(item,index){ %>
<tr cid="<%=item.id%>" uid="<%=item.uid%>" sid="<%=item.sid%>" fid="<%=item.fid%>" dname="<%=item.dname%>" access="<%=item.access%>" class="<%=index%2==0 ? '':'gray' %>">
    <td class="th1"><%=item.access%></td>
    <td class="th2"><%=item.uid%></td>
    <td class="th3"><%=item.dname%></td>
    <td class="th4"  uid="<%=item.uid%>"  sid="<%=item.sid%>" fid="<%=item.fid%>" dname="<%=item.dname%>" access="<%=item.access%>"><a href="javascript:void(0)" class="delete_fun">删除</a></td>
</tr>
<% } )%>