<% _.each(data,function(item,index){ %>
<tr>
    <td class="th1"><%=item.excel_type.desc%></td>
    <td class="th2"><%=item.create_time%></td>
    <td class="th3"><%=item.done == 1? "已生成" : "生成中"%></td>
    <td class="th4"><%=item.download%>下载次数</td>
    <td class="th6 down_href <%=item.done == 1? "" : "disabled"%>">
        <a class="<%=item.done == 1? "" : "disabled"%>" href="javascript:void(0)">下载</a>
    </td>
</tr>
<% } ) %>