<% _.each( data, function( item, index ) {%>
                <tr>
                    <td><%=item.name%></td>
                    <td><%=item.mode%></td>
                    <td><%=item.fee%></td>
                    <td><%=item.begin_time%> 至 <%=item.end_time%></td>
                    <td><a href="appcenter_chargesetting.html?id=<%=item.id%>" class="btn-link mr10">编辑</a> <a data-id="<%=item.id%>" href="javascript:;" class="btn-link btnRemoveCharge">下架</a></td>
                </tr>
<% }) %>