<%_.each(data,function(item,index){%>
    <%_.each(item.tickets,function(ticket,idx){%>
        <li class="item" data-sid="<%=ticket.apply_sid%>" data-tid="<%=ticket.tid%>">
            <span class="t"><%=ticket.ltitle%>【<%=ticket.ttitle%>】</span>
            <span data-sid="<%=ticket.apply_sid%>" data-tid="<%=ticket.tid%>" class="addBtn">添加</span>
        </li>
    <%})%>
<%})%>