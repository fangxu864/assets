<% _.each(data.list,function(item,index){ %>
<tr>
    <td class="th1"><%=data.name_land%></td>
    <td class="th2"><%=item.name%></td>
    <td class="th3"><%=item.id_card%></td>
    <td class="th4">
        <span class="edit-btn" data-id="<%=item.id%>" data-lid="<%=item.lid%>" data-name="<%=item.name%>" data-idNum="<%=item.id_card%>">编辑</span>
        <span class="delete-btn" data-id="<%=item.id%>" data-lid="<%=item.lid%>">删除</span>
    </td>
</tr>
<%})%>