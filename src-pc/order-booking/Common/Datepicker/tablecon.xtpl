<% _.each(data.list,function(item,index){ %>
    <tr>
    <% for( var i = 0; i < item.length; i++){%>
        <td class="disable <%=item[i].date == data.curDay ? 'selected' : ''%>" data-date="<%=item[i].date%>">
            <div style="width: 100%;height: 100%;position: relative">
                <p class="date <%=item[i].today == 'today' ? 'today' : ''%>"><%=item[i].today == 'today' ? 'ä»Šå¤©' : item[i].day%></p>
                <p class="price"><%=item[i].date ? '' : ''%></p>
            </div>

        </td>
    <%}%>
    </tr>
<%})%>

