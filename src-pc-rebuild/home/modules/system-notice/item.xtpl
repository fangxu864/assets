<%if(data.length == 0 && data.length == false){%>
    <li>暂无相关通知</li>
<%}else{%>
    <% $.each(data,function(index,value){     %>
        <li>
            <a href="pft_announce.html?id=<%=value.id%>&m=con" class="noticeTitle" id="<%=value.id%>" title="<%=value.title%>"><%=value.title%></a><span class="noticeTime"><%=value.create_time%></span>
        </li>
    <% }) %>
<%}%>